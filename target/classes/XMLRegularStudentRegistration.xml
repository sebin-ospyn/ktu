				</methods>
			</class>
		</classes>
	</test>
	
	<test name="TC_RegularStudentRegistration_07-Approving from university login">		
		<classes>
			<class name="com.ospyn.ktu.test.RegularStudentRegistration">
				<methods>					
				<include name="Login_as_University_User" />	
				<include name="click_Student_Module" />	
				<include name="university_Approving_Student_Details" />														
				</methods>
			</class>
		</classes>
